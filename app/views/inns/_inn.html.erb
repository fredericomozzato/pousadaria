<div class="inn-card">
    <h3><%= link_to inn.name, inn %></h3>
    <% if inn.photos.attached? %>
        <div class="photo-container">
            <%= link_to image_tag(inn.photos.first.variant(resize_to_limit: [300, 300]).processed), inn %>
        </div>
    <% end %>
    <p><%= inn.address.city %> - <%= inn.address.state %></p>
</div>