<%= form_with model: @seasonal_price, url: [@room, @seasonal_price] do |f| %>
  <div>
    <%= f.label :start %>
    <%= f.date_field :start %>
  </div>
  <div>
    <%= f.label :end %>
    <%= f.date_field :end %>
  </div>
  <div>
    <%= f.label :price %>
    <%= f.number_field :price, step: 0.01 %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>